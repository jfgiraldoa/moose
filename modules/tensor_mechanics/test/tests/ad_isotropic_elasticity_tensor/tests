  [../]

  [./lambda_shear-jac]
    type = PetscJacobianTester
    input = 'lambda_shear_modulus_test.i'
    run_sim = 'True'
    ratio_tol = 1e-8
    difference_tol = 1e-5
    petsc_version = '>=3.9.4'
    issues = '#12650'
    design = 'jacobian_definitions.md'
    requirement = 'The ComputeIsotropicElasticityTensor class shall correctly compute the elasticity tensor from the lambda and shear modulus for an isotropic material using AD formulations and produce a perfect Jacobian.'
