[Tests]
  issues = '#4783'
  design = 'ComputeIsotropicElasticityTensor.md'

  [./lambda_shear]
    type = Exodiff
    input = 'lambda_shear_modulus_test.i'
    exodiff = 'lambda_shear_modulus_test_out.e'
    requirement = 'The ComputeIsotropicElasticityTensor class shall correctly compute the elasticity tensor from the lambda and shear modulus for an isotropic material using AD formulations.'
  [../]
  [./youngs_poissons]
    type = Exodiff
